{
    "name": "units-of-measure",
    "version": "1.0.0",
    "scripts": {
        "build": "ng build",
        "build.prod": "ng run units-of-measure:prerender",
        "deploy": "ts-node --project scripts/tsconfig.deploy.json scripts/deploy.ts",
        "e2e": "ng e2e",
        "e2e.ci": "ng e2e --headless",
        "format": "prettier --write .",
        "lint": "ng run units-of-measure:eslint",
        "ng": "ng",
        "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
        "release": "ts-node --project scripts/tsconfig.deploy.json scripts/release.ts",
        "start": "ng serve",
        "start.prod": "ng run units-of-measure:serve-ssr",
        "test": "ng test",
        "test.ci": "ng test -ci --runInBand",
        "test.watch": "ng test --watchAll"
    },
    "private": true,
